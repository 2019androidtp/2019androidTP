<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.apicollaboration">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".Activity.SearchAllMarkerActivity"
                android:label="@string/title_activity_search_all_marker"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".Activity.DaumSearchActivity"
                android:label="@string/title_activity_daum_search"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Activity.AddressMarkAcitvity"/>
        <activity
                android:name=".Activity.ListViewActivity"
                android:label="@string/title_activity_list_view"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>