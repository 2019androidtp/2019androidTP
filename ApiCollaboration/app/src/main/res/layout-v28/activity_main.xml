    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <ImageView
            android:layout_width="240dp"
            android:layout_height="124dp"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:contentDescription="TODO"
            android:src="@drawable/orangecorner" />


        <Button
            android:id="@+id/nameA"
            android:layout_width="48dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10.7dp"
            android:layout_marginTop="134.7dp"
            android:background="@drawable/radiusblack"
            tools:text="A"
            android:textColor="@color/white"
             />

        <Button
            android:id="@+id/searchBtn"
            android:layout_width="165.3dp"
            android:layout_height="24dp"
            android:layout_marginStart="64dp"
            android:layout_marginTop="134.7dp"
            android:background="@drawable/radiuswhite"
            android:text="주소로 검색" />

        <Button
            android:id="@+id/nameB"
            android:layout_width="48dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10.7dp"
            android:layout_marginTop="172dp"
