            tools:text="B"
            android:textColor="@color/white"
             />

        <Button
            android:id="@+id/searchDaumBtn"
            android:layout_width="165.3dp"
            android:layout_height="24dp"
            android:layout_marginLeft="64dp"
            android:layout_marginTop="172dp"
            android:background="@drawable/radiuswhite"
            android:text="다음주소로 검색" />

        <Button
            android:id="@+id/nameC"
            android:layout_width="48dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10.7dp"
            android:layout_marginTop="209.3dp"
            android:background="@drawable/radiusblack"
            tools:text="C"
            android:textColor="@color/white"
             />

        <Button
            android:id="@+id/searchAllMarkerBtn"
            android:layout_width="165.3dp"
            android:layout_height="24dp"
            android:layout_marginLeft="64dp"
            android:layout_marginTop="209.3dp"
            android:background="@drawable/radiuswhite"
            android:text="모든마커 검색" />


